{
  "name": "abogado",
  "version": "1.0.4",
  "description":
    "Checks a package's dependencies for compliance with a specified licensing policy.",
  "author": "Gary DeReese Jr",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gdereese/abogado.git"
  },
  "bugs": {
    "url": "https://github.com/gdereese/abogado/issues"
  },
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "files": ["dist/*.js"],
  "scripts": {
    "build": "npm run-script lint && tsc",
    "coverage": "nyc -r lcov -e .ts -x \"*.spec.ts\" npm test",
    "license-check": "node .",
    "lint": "tslint --project tsconfig.json",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run-script lint && tsc",
    "preversion": "npm test",
    "version": "npm run-script build",
    "test": "jasmine-ts test/**/*.spec.ts"
  },
  "devDependencies": {
    "@types/jasmine": "2.8.6",
    "@types/lodash": "4.14.98",
    "@types/node": "8.5.9",
    "jasmine-ts": "0.2.1",
    "nyc": "11.6.0",
    "prettier": "1.10.2",
    "sonar-scanner": "3.0.3",
    "tslint": "5.9.1",
    "tslint-config-prettier": "1.6.0",
    "typescript": "2.6.2"
  },
  "dependencies": {
    "chalk": "2.3.0",
    "commander": "2.13.0",
    "lodash": "4.17.4"
  }
}
