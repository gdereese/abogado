{
  "name": "abogado",
  "version": "1.0.4",
  "description":
    "Checks a package's dependencies for compliance with a specified licensing policy.",
  "keywords": ["nodejs", "licenses"],
  "author": "Gary DeReese Jr",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gdereese/abogado.git"
  },
  "bugs": {
    "url": "https://github.com/gdereese/abogado/issues"
  },
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "files": ["dist/*.js"],
  "scripts": {
    "build": "npm run-script lint && babel src -d dist",
    "coverage": "nyc -r lcov -e .js -x \"*.spec.js\" npm test",
    "license-check": "node .",
    "lint": "eslint src/** test/**",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run-script lint && tsc",
    "preversion": "npm test",
    "version": "npm run-script build",
    "test": "jasmine test/**/*.spec.js"
  },
  "devDependencies": {
    "nyc": "11.6.0",
    "babel-cli": "6.26.0",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-jasmine": "2.9.3",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-standard": "3.0.1",
    "jasmine": "3.1.0",
    "prettier": "1.11.1",
    "sonar-scanner": "3.0.3"
  },
  "dependencies": {
    "chalk": "2.3.0",
    "commander": "2.13.0",
    "lodash": "4.17.4"
  }
}
